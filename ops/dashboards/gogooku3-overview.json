{
  "dashboard": {
    "id": null,
    "title": "gogooku3-standalone Overview",
    "tags": ["gogooku3", "ml", "finance"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "Application Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"gogooku3\"}",
            "legendFormat": "{{job}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "System Memory Usage",
        "type": "gauge",
        "targets": [
          {
            "expr": "gogooku3_memory_usage_percent",
            "legendFormat": "Memory Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 85
                },
                {
                  "color": "red",
                  "value": 95
                }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "CPU Usage",
        "type": "gauge",
        "targets": [
          {
            "expr": "gogooku3_cpu_usage_percent",
            "legendFormat": "CPU Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 80
                },
                {
                  "color": "red",
                  "value": 95
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Disk Usage",
        "type": "bargauge",
        "targets": [
          {
            "expr": "gogooku3_disk_usage_percent",
            "legendFormat": "Disk Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 85
                },
                {
                  "color": "red",
                  "value": 95
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Application Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "gogooku3_uptime_seconds",
            "legendFormat": "Uptime"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "mappings": []
          }
        }
      },
      {
        "id": 6,
        "title": "Log Files Overview",
        "type": "table",
        "targets": [
          {
            "expr": "gogooku3_log_files_count",
            "legendFormat": "Log Files Count"
          },
          {
            "expr": "gogooku3_log_files_size_bytes",
            "legendFormat": "Log Files Size"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      },
      {
        "id": 7,
        "title": "Data Files Overview",
        "type": "table",
        "targets": [
          {
            "expr": "gogooku3_data_files_count",
            "legendFormat": "Data Files Count"
          },
          {
            "expr": "gogooku3_data_files_size_bytes",
            "legendFormat": "Data Files Size"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      },
      {
        "id": 8,
        "title": "Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(gogooku3_network_bytes_sent[5m])",
            "legendFormat": "Bytes Sent/sec"
          },
          {
            "expr": "rate(gogooku3_network_bytes_recv[5m])",
            "legendFormat": "Bytes Received/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "Bps"
          }
        }
      },
      {
        "id": 9,
        "title": "Process Memory",
        "type": "graph",
        "targets": [
          {
            "expr": "gogooku3_process_memory_bytes",
            "legendFormat": "Process Memory"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      },
      {
        "id": 10,
        "title": "Process CPU",
        "type": "graph",
        "targets": [
          {
            "expr": "gogooku3_process_cpu_percent",
            "legendFormat": "Process CPU %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
