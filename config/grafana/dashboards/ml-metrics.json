{
  "dashboard": {
    "id": null,
    "uid": "gogooku3-ml",
    "title": "Gogooku3 ML Metrics",
    "tags": ["gogooku3", "ml", "mlflow"],
    "timezone": "Asia/Tokyo",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "type": "graph",
        "title": "Model Training Loss",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "mlflow_training_loss",
            "legendFormat": "{{model_name}} - {{stage}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "show": true },
          { "show": false }
        ],
        "lines": true,
        "linewidth": 2
      },
      {
        "id": 2,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "type": "graph",
        "title": "Model Accuracy",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "mlflow_model_accuracy",
            "legendFormat": "{{model_name}} - {{dataset}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "percentunit", "show": true, "min": 0, "max": 1 },
          { "show": false }
        ]
      },
      {
        "id": 3,
        "gridPos": { "h": 6, "w": 6, "x": 0, "y": 8 },
        "type": "stat",
        "title": "Active Models",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "count(mlflow_registered_models)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 4,
        "gridPos": { "h": 6, "w": 6, "x": 6, "y": 8 },
        "type": "stat",
        "title": "Production Models",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "count(mlflow_model_stage{stage=\"Production\"})",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 5,
        "gridPos": { "h": 6, "w": 6, "x": 12, "y": 8 },
        "type": "stat",
        "title": "Avg Inference Time",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "avg(mlflow_inference_duration_seconds)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "unit": "ms"
      },
      {
        "id": 6,
        "gridPos": { "h": 6, "w": 6, "x": 18, "y": 8 },
        "type": "stat",
        "title": "Total Predictions",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(mlflow_predictions_total[1h])) * 3600",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "unit": "short"
      },
      {
        "id": 7,
        "gridPos": { "h": 10, "w": 12, "x": 0, "y": 14 },
        "type": "graph",
        "title": "Feature Importance",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "topk(10, mlflow_feature_importance)",
            "legendFormat": "{{feature_name}}",
            "refId": "A"
          }
        ],
        "bars": true,
        "lines": false,
        "yaxes": [
          { "format": "percentunit", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 8,
        "gridPos": { "h": 10, "w": 12, "x": 12, "y": 14 },
        "type": "table",
        "title": "Model Registry",
        "datasource": "ClickHouse",
        "targets": [
          {
            "rawSql": "SELECT model_name, version, stage, accuracy, created_at FROM ml_models ORDER BY created_at DESC LIMIT 20",
            "refId": "A"
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "created_at",
              "desc": true
            }
          ]
        }
      },
      {
        "id": 9,
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 24 },
        "type": "heatmap",
        "title": "Prediction Distribution",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum(rate(mlflow_prediction_histogram_bucket[5m])) by (le))",
            "format": "heatmap",
            "refId": "A"
          }
        ],
        "options": {
          "calculate": true,
          "yAxis": {
            "unit": "short",
            "decimals": 0
          },
          "color": {
            "mode": "spectrum",
            "scheme": "interpolateSpectral"
          }
        }
      }
    ]
  }
}
