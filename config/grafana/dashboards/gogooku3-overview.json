{
  "dashboard": {
    "id": null,
    "uid": "gogooku3-overview",
    "title": "Gogooku3 System Overview",
    "tags": ["gogooku3", "overview"],
    "timezone": "Asia/Tokyo",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "type": "graph",
        "title": "System CPU Usage",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "percent", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 2,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "type": "graph",
        "title": "Memory Usage",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "megabytes", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 3,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "type": "stat",
        "title": "Dagster Pipeline Status",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "dagster_run_success_total",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 4,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "type": "graph",
        "title": "Data Processing Rate",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(data_processed_rows_total[5m])",
            "legendFormat": "Rows/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "ops", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 5,
        "gridPos": { "h": 8, "w": 8, "x": 0, "y": 16 },
        "type": "stat",
        "title": "Redis Cache Hit Rate",
        "datasource": "Redis",
        "targets": [
          {
            "query": "GET cache_hit_rate",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "green", "value": 90 }
            ]
          }
        }
      },
      {
        "id": 6,
        "gridPos": { "h": 8, "w": 8, "x": 8, "y": 16 },
        "type": "table",
        "title": "ClickHouse Tables",
        "datasource": "ClickHouse",
        "targets": [
          {
            "rawSql": "SELECT table, formatReadableSize(sum(bytes)) as size, sum(rows) as rows FROM system.parts WHERE database = 'gogooku3' GROUP BY table",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "gridPos": { "h": 8, "w": 8, "x": 16, "y": 16 },
        "type": "stat",
        "title": "MinIO Storage Usage",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "minio_disk_storage_used_bytes / 1024 / 1024 / 1024",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "unit": "GB"
      }
    ]
  }
}
