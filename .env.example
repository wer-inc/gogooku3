# JQuants API credentials
JQUANTS_EMAIL=your_email@example.com
JQUANTS_PASSWORD=your_password

# Storage configuration
MINIO_ENDPOINT=http://localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_SECURE=false

# ClickHouse configuration
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=8123
CLICKHOUSE_DATABASE=gogooku3
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=

# Redis configuration (for Feast online store)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Dagster configuration
DAGSTER_HOME=/opt/dagster/dagster_home
DAGSTER_PG_HOST=localhost
DAGSTER_PG_PORT=5432
DAGSTER_PG_USERNAME=dagster
DAGSTER_PG_PASSWORD=dagster
DAGSTER_PG_DB=dagster

# Processing configuration
MAX_CONCURRENT_FETCH=150
MAX_PARALLEL_WORKERS=24

# Monitoring
PROMETHEUS_PORT=8000
GRAFANA_PORT=3001

# Environment
ENVIRONMENT=development
