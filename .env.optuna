# ATFT-GAT-FAN Optuna Optimization Environment Variables

# MLflow Configuration
MLFLOW=1
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_EXPERIMENT=ATFT-GAT-FAN-Optuna
MLFLOW_RUN_NAME_PREFIX=optuna_trial

# CUDA Memory Management
PYTORCH_CUDA_ALLOC_CONF=expandable_segments:True

# Optuna Trial Settings
OPTUNA_TRIAL=1
SMOKE_DATA_MAX_FILES=100
MINIMAL_COLUMNS=1

# Logging Configuration
LOG_TZ=Asia/Tokyo
TZ=Asia/Tokyo

# JQuants API (if needed for data refresh)
# JQUANTS_AUTH_EMAIL=your_email@example.com
# JQUANTS_AUTH_PASSWORD=your_password

# Performance Optimizations
OMP_NUM_THREADS=8
MKL_NUM_THREADS=8
NUMBA_NUM_THREADS=8