{
  "dashboard": {
    "id": null,
    "uid": "gogooku3-pipeline",
    "title": "Gogooku3 Data Pipeline Monitor",
    "tags": ["gogooku3", "pipeline", "dagster"],
    "timezone": "Asia/Tokyo",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
        "type": "stat",
        "title": "Pipeline Success Rate",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(dagster_run_success_total[1h]) / rate(dagster_run_total[1h]) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 99 }
            ]
          }
        },
        "unit": "percent"
      },
      {
        "id": 2,
        "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
        "type": "stat",
        "title": "Active Runs",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "dagster_runs_in_progress",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "colorMode": "background",
          "graphMode": "none"
        }
      },
      {
        "id": 3,
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
        "type": "stat",
        "title": "Failed Runs (24h)",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "increase(dagster_run_failure_total[24h])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      },
      {
        "id": 4,
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
        "type": "stat",
        "title": "Avg Pipeline Duration",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "avg(dagster_run_duration_seconds)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "unit": "s"
      },
      {
        "id": 5,
        "gridPos": { "h": 10, "w": 12, "x": 0, "y": 4 },
        "type": "graph",
        "title": "Pipeline Execution Timeline",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(dagster_run_total[5m])",
            "legendFormat": "Total",
            "refId": "A"
          },
          {
            "expr": "rate(dagster_run_success_total[5m])",
            "legendFormat": "Success",
            "refId": "B"
          },
          {
            "expr": "rate(dagster_run_failure_total[5m])",
            "legendFormat": "Failed",
            "refId": "C"
          }
        ],
        "yaxes": [
          { "format": "ops", "show": true },
          { "show": false }
        ],
        "lines": true,
        "linewidth": 2
      },
      {
        "id": 6,
        "gridPos": { "h": 10, "w": 12, "x": 12, "y": 4 },
        "type": "graph",
        "title": "Asset Materialization",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "increase(dagster_asset_materialization_total[1h])",
            "legendFormat": "{{asset_key}}",
            "refId": "A"
          }
        ],
        "bars": true,
        "lines": false,
        "stack": true,
        "yaxes": [
          { "format": "short", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 7,
        "gridPos": { "h": 8, "w": 8, "x": 0, "y": 14 },
        "type": "table",
        "title": "Recent Pipeline Runs",
        "datasource": "ClickHouse",
        "targets": [
          {
            "rawSql": "SELECT run_id, pipeline_name, status, started_at, duration_sec FROM pipeline_runs ORDER BY started_at DESC LIMIT 10",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "gridPos": { "h": 8, "w": 8, "x": 8, "y": 14 },
        "type": "piechart",
        "title": "Job Distribution",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum by (job_name) (dagster_job_executions_total)",
            "legendFormat": "{{job_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "donut",
          "legendDisplayMode": "list",
          "legendPlacement": "right"
        }
      },
      {
        "id": 9,
        "gridPos": { "h": 8, "w": 8, "x": 16, "y": 14 },
        "type": "graph",
        "title": "Data Volume Processed",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(increase(data_rows_processed_total[1h]))",
            "legendFormat": "Rows/hour",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "show": true, "logBase": 10 },
          { "show": false }
        ],
        "bars": true,
        "lines": false
      },
      {
        "id": 10,
        "gridPos": { "h": 6, "w": 24, "x": 0, "y": 22 },
        "type": "alertlist",
        "title": "Active Alerts",
        "options": {
          "showOptions": "current",
          "maxItems": 10,
          "sortOrder": 1,
          "dashboardAlerts": true,
          "alertName": "",
          "dashboardTitle": "",
          "tags": ["pipeline", "dagster"]
        }
      }
    ]
  }
}
